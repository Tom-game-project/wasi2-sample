{
  "presets": [
    ["@babel/preset-env", {
      "targets": { "chrome": "58" },
      "debug": true,
      "useBuiltIns": false,
      "forceAllTransforms": true
    }]
  ],
  "plugins": [
	  "babel-plugin-transform-bigint-to-jsbi"
  ],
  "assumptions": {
    "privateFieldsAsProperties": true,
    "setPublicClassFields": true
  }
}

